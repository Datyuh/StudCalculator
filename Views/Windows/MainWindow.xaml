<Window
    x:Class="StudCalculator.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:DBWork="clr-namespace:StudCalculator.DBWork"
    xmlns:cmd="clr-namespace:StudCalculator.Infrastructure.Commands"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:StudCalculator"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="clr-namespace:StudCalculator.ViewModel"
    Title="{Binding Title}"
    Width="860"
    Height="580"
    FontFamily="Calibri"
    Icon="Stud_Calculator.ico"
    Loaded="Window_Loaded"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Window.DataContext>
        <vm:MainWindowViewModel />
    </Window.DataContext>
    <Grid Background="#FFCFCFCF">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="418*" />
            <ColumnDefinition Width="421*" />
            <ColumnDefinition Width="10" />
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="5" />
            <RowDefinition Height="184" />
            <RowDefinition Height="298" />
            <RowDefinition />
            <RowDefinition Height="10" />
        </Grid.RowDefinitions>

        <Label
            Grid.Row="1"
            Width="157"
            Height="30"
            Margin="5,5,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите ГОСТ фланцев"
            FontFamily="Calibri"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Width="230"
            Height="30"
            Margin="5,40,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите тип для ГОСТ 33259-2015"
            FontFamily="Calibri"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Width="143"
            Height="30"
            Margin="5,75,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите давление"
            FontFamily="Calibri"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Width="157"
            Height="30"
            Margin="0,112,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите исполнение"
            FontFamily="Calibri"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Width="97"
            Height="30"
            Margin="5,147,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите DN"
            FontFamily="Calibri"
            FontSize="14"
            RenderTransformOrigin="0.522,2.703" />

        <ComboBox
            x:Name="Type_GOST33259_Combo"
            Grid.Row="1"
            Width="178"
            Height="30"
            Margin="230,40,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            FontSize="14"
            ItemsSource="{Binding ExecutionType33259}"
            RenderTransformOrigin="0.705,0.522">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <ComboBox
            x:Name="Execution_PN_Combo"
            Grid.Row="1"
            Width="178"
            Height="30"
            Margin="230,75,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            FontSize="14"
            ItemsSource="{Binding ExecutionPN33259}"
            RenderTransformOrigin="0.458,1.733">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <ComboBox
            x:Name="Exec_GOST33259"
            Grid.Row="1"
            Width="178"
            Height="30"
            Margin="230,112,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            FontSize="14"
            ItemsSource="{Binding ExecGost33259, Mode=TwoWay}">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <ComboBox
            x:Name="Execution_DN_Combo"
            Grid.Row="1"
            Width="178"
            Height="30"
            Margin="230,147,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            FontSize="14"
            ItemsSource="{Binding ExecutionDN33259}">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>
        <ComboBox
            x:Name="GOSTall"
            Grid.Row="1"
            Width="178"
            Height="30"
            Margin="230,5,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            FontSize="14"
            ItemsSource="{Binding AllGost, Mode=TwoWay}"
            SelectedValue="{Binding SelectionFromCombobox, Mode=TwoWay}">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <TextBlock
            Grid.Row="2"
            Width="180"
            Height="17"
            Margin="107,5,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Calibri"
            FontSize="14"
            Text="Если фланецы нестандартные"
            TextWrapping="Wrap" />
        <CheckBox
            x:Name="NonStandartFl"
            Grid.Row="2"
            Width="167"
            Height="17"
            Margin="5,27,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Command="{Binding NonStandartSameFlangeCommand, Mode=OneWay}"
            Content="Нестандартный фланец"
            FontFamily="Calibri"
            FontSize="14"
            IsChecked="{Binding NonStandartSameFlangChecked}" IsEnabled="{Binding NonStandartStandartSameFlangCheckedIsEnabled}" />
        <CheckBox
            x:Name="NonStandartDablFl"
            Grid.Row="2"
            Width="178"
            Height="17"
            Margin="230,27,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Command="{Binding NonStandartDifferentFlangeCommand, Mode=OneWay}"
            Content="Если разные тарелки"
            FontFamily="Calibri"
            FontSize="14"
            IsChecked="{Binding NonStandartDifferentFlangeChecked}" IsEnabled="{Binding NonStandartStandartDifferentFlangCheckedIsEnabled}" />

        <TextBlock
            Grid.Row="2"
            Width="242"
            Height="17"
            Margin="77,78,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Calibri"
            FontSize="14"
            Text="Если используется заглушка поворотная"
            TextWrapping="Wrap" />
        <CheckBox
            x:Name="StandardRotaryPlug"
            Grid.Row="2"
            Width="178"
            Height="17"
            Margin="230,100,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Стандартные"
            FontFamily="Calibri"
            FontSize="14" />
        <CheckBox
            x:Name="NonStandardRotaryPlug"
            Grid.Row="2"
            Width="153"
            Height="17"
            Margin="5,100,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Нестандартные"
            FontFamily="Calibri"
            FontSize="14" />

        <TextBlock
            Grid.Row="2"
            Width="242"
            Height="18"
            Margin="77,157,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Calibri"
            FontSize="14"
            TextWrapping="Wrap"><Run Text="Если используется заглушка " /><Run Text="или крышка" /></TextBlock>
        <CheckBox
            x:Name="NonStandardPlug"
            Grid.Row="2"
            Width="153"
            Height="18"
            Margin="5,180,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Command="{Binding NonstandartPlugsCommand, Mode=OneWay}"
            Content="Нестандартные"
            FontFamily="Calibri"
            FontSize="14"
            IsChecked="{Binding NonStandartPlugsChecked}"
            IsEnabled="{Binding NonStandartPlugsCheckboxIsEnabled}" />
        <CheckBox
            x:Name="StandardPlug"
            Grid.Row="2"
            Width="178"
            Height="18"
            Margin="230,180,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Command="{Binding StandartPlugsCommand, Mode=OneWay}"
            Content="Стандартные"
            FontFamily="Calibri"
            FontSize="14"
            IsChecked="{Binding StandartPlugsChecked}"
            IsEnabled="{Binding StandartPlugsCheckboxIsEnabled}" />

        <ComboBox
            x:Name="StandardRotaryPlug_Combo"
            Grid.Row="2"
            Width="178"
            Height="30"
            Margin="230,122,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <Border
            Grid.Row="2"
            Width="80"
            Height="24"
            Margin="28,261,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="NumberFlanges_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri" />
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="шт."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <ComboBox
            x:Name="StandardPlug_Combo"
            Grid.Row="2"
            Width="178"
            Height="30"
            Margin="230,203,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            IsEnabled="{Binding StandartPlugsComboboxIsEnabled}">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <ComboBox
            x:Name="ExecutionStubs_Combo"
            Grid.Row="2"
            Width="178"
            Height="30"
            Margin="230,261,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman" IsEnabled="{Binding StandartPlugsExecutionComboboxIsEnabled}">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <TextBlock
            Grid.Row="2"
            Width="178"
            Height="18"
            Margin="230,238,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Calibri"
            FontSize="14"
            Text="Исполнение загл. и крышек"
            TextAlignment="Center"
            TextWrapping="Wrap" />
        <TextBlock
            Grid.Row="2"
            Width="113"
            Height="18"
            Margin="5,238,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Calibri"
            FontSize="14"
            Text="Кол-во фланцев"
            TextWrapping="Wrap" />

        <Border
            Grid.Row="2"
            Width="80"
            Height="24"
            Margin="28,203,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="NonStandardPlug_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri"
                    IsEnabled="{Binding NonStandartPlugsTextboxIsEnabled}" />
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Border
            Grid.Row="2"
            Width="80"
            Height="24"
            Margin="28,122,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="StandardRotaryPlug_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri" />
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Border
            Grid.Row="2"
            Width="80"
            Height="24"
            Margin="28,49,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="NonStandartFl_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri"
                    IsEnabled="{Binding NonStandartFlTextIsEnabled}"
                    Text="{Binding NonStandartFlText, Mode=OneWayToSource}">
                    <TextBox.ToolTip>
                        <TextBlock><Run Text="Указывать высоту тарелки до прокладки кроме овальных" /></TextBlock>
                    </TextBox.ToolTip>
                </TextBox>
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Border
            Grid.Row="2"
            Width="80"
            Height="24"
            Margin="230,49,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="NonStandartDablFl_TextFirst"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri"
                    IsEnabled="{Binding NonStandartDifferentFlangeTexboxIsEnabled}">
                    <TextBox.ToolTip>
                        <TextBlock><Run Text="Указывать высоту тарелки до прокладки кроме овальных" /></TextBlock>
                    </TextBox.ToolTip>
                </TextBox>
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Border
            Grid.Row="2"
            Width="80"
            Height="24"
            Margin="328,49,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1"
            IsEnabled="{Binding NonStandartDifferentFlangeTexboxIsInabled}">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="NonStandartDablFl_TextSec"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri"
                    IsEnabled="{Binding NonStandartDifferentFlangeTexboxIsEnabled}">
                    <TextBox.ToolTip>
                        <TextBlock><Run Text="Указывать высоту тарелки до прокладки кроме овальных" /></TextBlock>
                    </TextBox.ToolTip>
                </TextBox>
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Label
            Grid.Row="1"
            Grid.Column="1"
            Width="203"
            Height="30"
            Margin="10,5,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите ОСТ на гайки и шаг"
            FontFamily="Times New Roman"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Grid.Column="1"
            Width="203"
            Height="30"
            Margin="10,40,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Выберите резьбу гайки"
            FontFamily="Times New Roman"
            FontSize="14" />

        <ComboBox
            x:Name="OSTNuts_Combo"
            Grid.Row="1"
            Grid.Column="1"
            Width="178"
            Height="30"
            Margin="0,5,5,0"
            Padding="6"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            FontFamily="Times New Roman">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <ComboBox
            x:Name="NutThread_Combo"
            Grid.Row="1"
            Grid.Column="1"
            Width="178"
            Height="30"
            Margin="0,40,5,0"
            Padding="6"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            FontFamily="Times New Roman" IsEnabled="{Binding ChoeseNutsThreadComboboxIsEnable}">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <CheckBox
            x:Name="StandartThicknessWasher"
            Grid.Row="1"
            Grid.Column="1"
            Width="78"
            Height="17"
            Margin="103,105,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Стандарт"
            FontFamily="Calibri" />
        <CheckBox
            x:Name="StandartThicknessStripOval"
            Grid.Row="1"
            Grid.Column="1"
            Width="74"
            Height="17"
            Margin="244,105,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Овал."
            FontFamily="Calibri" />
        <CheckBox
            x:Name="NonStandartThicknessWasher"
            Grid.Row="1"
            Grid.Column="1"
            Width="88"
            Height="17"
            Margin="10,105,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Нестандарт"
            FontFamily="Calibri" />
        <CheckBox
            x:Name="StandartThicknessStripOrtag"
            Grid.Row="1"
            Grid.Column="1"
            Width="89"
            Height="17"
            Margin="325,105,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Восьмиг."
            FontFamily="Calibri" />

        <Border
            Grid.Row="1"
            Grid.Column="1"
            Width="80"
            Height="24"
            Margin="43,127,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="Washer_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri" />
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Border
            Grid.Row="1"
            Grid.Column="1"
            Width="80"
            Height="24"
            Margin="284,127,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="Strip_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri" />
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="мм."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <Label
            Grid.Row="1"
            Grid.Column="1"
            Width="170"
            Height="30"
            Margin="10,75,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            HorizontalContentAlignment="Center"
            VerticalContentAlignment="Center"
            Content="Толщина шайбы"
            FontFamily="Calibri"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Grid.Column="1"
            Width="170"
            Height="30"
            Margin="244,75,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            HorizontalContentAlignment="Center"
            VerticalContentAlignment="Center"
            Content="Тлощина прокладки"
            FontFamily="Calibri"
            FontSize="14" />
        <Label
            Grid.Row="1"
            Grid.Column="1"
            Width="90"
            Height="30"
            Margin="177,154,157,0"
            Padding="6"
            HorizontalAlignment="Center"
            VerticalAlignment="Top"
            Content="Шпильки"
            FontFamily="Times New Roman"
            FontSize="16"
            FontWeight="Bold" />

        <Border
            Grid.Row="2"
            Grid.Column="1"
            Width="80"
            Height="24"
            Margin="43,30,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1">
            <StackPanel Margin="0" Orientation="Horizontal">
                <TextBox
                    x:Name="NumberSud_Text"
                    Width="56"
                    BorderBrush="{x:Null}"
                    FontFamily="Calibri" />
                <TextBlock
                    Width="22"
                    Margin="0,3"
                    FontFamily="Calibri"
                    Text="шт."
                    TextAlignment="Center" />
            </StackPanel>
        </Border>

        <ComboBox
            x:Name="MaterialStud"
            Grid.Row="2"
            Grid.Column="1"
            Width="153"
            Height="30"
            Margin="0,27,0,0"
            Padding="6"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            FontFamily="Times New Roman"
            RenderTransformOrigin="0.503,0.033">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <ComboBox
            x:Name="ExecutionStud"
            Grid.Row="2"
            Grid.Column="1"
            Width="60"
            Height="30"
            Margin="192,27,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FontFamily="Times New Roman">
            <ComboBox.Effect>
                <DropShadowEffect />
            </ComboBox.Effect>
        </ComboBox>

        <Label
            Grid.Row="2"
            Grid.Column="1"
            Width="106"
            Height="30"
            Margin="26,0,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Кол-во шпилек"
            FontFamily="Times New Roman"
            FontSize="14" />
        <Label
            Grid.Row="2"
            Grid.Column="1"
            Width="85"
            Height="30"
            Margin="177,0,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Исполнение"
            FontFamily="Times New Roman"
            FontSize="14"
            RenderTransformOrigin="0.941,0.433" />
        <Label
            Grid.Row="2"
            Grid.Column="1"
            Width="152"
            Height="30"
            Margin="272,0,0,0"
            Padding="6"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            HorizontalContentAlignment="Center"
            Content="Материал"
            FontFamily="Times New Roman"
            FontSize="14" />

        <TextBox
            x:Name="OutputResult"
            Grid.Row="2"
            Grid.Column="1"
            Margin="0,62,0,0"
            Background="White"
            BorderBrush="#FF71DC60"
            BorderThickness="3"
            FontFamily="Calibri"
            FontSize="14"
            Foreground="Black"
            IsReadOnly="True"
            TextWrapping="Wrap" />

        <Button
            x:Name="Calculate"
            Grid.Row="3"
            Grid.Column="1"
            Height="33"
            Margin="276,10,0,0"
            VerticalAlignment="Top"
            BorderBrush="#FFCF4646"
            BorderThickness="3"
            Content="Расчитать"
            FontSize="14" />
        <Button
            x:Name="ClearOutputResult"
            Grid.Row="3"
            Grid.Column="1"
            Width="178"
            Height="35"
            Margin="10,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Очистка поля результаттов"
            FontSize="14"
            RenderTransformOrigin="-0.017,0.6" />
        <Button
            x:Name="AboutProgram"
            Grid.Row="3"
            Width="123"
            Height="33"
            Margin="5,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="О программе"
            FontSize="14" />

    </Grid>
</Window>
